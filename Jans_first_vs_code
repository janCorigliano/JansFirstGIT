import pandas as pd
import matplotlib.pyplot as plt
import numpy as no

x = [1,2,3,4,5]
y = [2,4,6,8,10]

plt.plot(x, y)

plt.title('I - V curve  Twenty Degrees tilt angle', fontdict= {'Fontsize': 15})

plt.xlabel('Array Current this is a change (A)', fontdict= {'Fontsize': 15})
plt.ylabel('Array Voltage (V) these are some extra changes', fontdict= {'Fontsize': 15})

plt.grid() #dont actually need this, is the default b=True, which = 'major', axis = 'both'

#plt.savefig('Twenty Degrees tilt angle')

plt.show()
